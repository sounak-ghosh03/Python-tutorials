import matplotlib.pyplot as plt

# Data
values = [23, 45, 56, 78, 33]
categories = ['Category 1', 'Category 2', 'Category 3', 'Category 4', 'Category 5']

# Create pie chart
plt.pie(values, labels=categories, autopct='%1.1f%%')

# Add title
plt.title('Pie Chart Example')

# Display the plot
plt.show()